<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TokenServiceService WSDL service -->

  <service name="TokenService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://services.tokenserver.arste.pro"/>
      <parameter name="wsdlServiceElement" value="TokenServiceService"/>
      <parameter name="schemaQualified" value="http://services.tokenserver.arste.pro"/>
      <parameter name="wsdlServicePort" value="TokenService"/>
      <parameter name="className" value="pro.arste.tokenserver.services.TokenServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TokenService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="generateToken" qname="operNS:generateToken" xmlns:operNS="http://services.tokenserver.arste.pro" returnQName="retNS:generateTokenReturn" xmlns:retNS="http://services.tokenserver.arste.pro" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:firstName" xmlns:pns="http://services.tokenserver.arste.pro" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:lastName" xmlns:pns="http://services.tokenserver.arste.pro" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:ubn" xmlns:pns="http://services.tokenserver.arste.pro" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="generateToken"/>

  </service>
</deployment>
